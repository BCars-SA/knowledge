{
    "name": "my-super-service",
    "version": "1.0.0",
    "description": "Backend service for My Super Service using Bun and Express",
    "type": "module",
    "main": "dist/index.js",
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "build": "bun build src/index.ts --outdir dist --target node",
        "start": "bun run dist/index.js",
        "test": "bun test",
        "test:watch": "bun test --watch",
        "test:integration": "bun test tests/integration",
        "test:load": "bun test tests/load",
        "lint": "bunx biome lint",
        "lint:fix": "bunx biome lint --write",
        "lint:scripts:fix": "bunx biome lint scripts --fix",
        "type-check": "bun tsc --noEmit",
        "precommit": "bun type-check && bun lint:fix && bun test",
        "docker:build": "docker build -t my-super-service:latest .",
        "docker:dev": "docker-compose up -d",
        "docker:prod": "docker-compose -f docker-compose-production.yml up -d",
        "docker:prod:build": "docker-compose -f docker-compose-production.yml up -d --build",
        "docker:down": "docker-compose down",
        "docker:prod:down": "docker-compose -f docker-compose-production.yml down"
    },
    "dependencies": {
        "@artemkdr/core": "^0.0.2",
        "@artemkdr/langchainjs-patches": "^0.0.2",
        "@langchain/anthropic": "^0.3.28",
        "@langchain/community": "^0.3.53",
        "@langchain/core": "^0.3.72",
        "@langchain/google-vertexai": "^0.2.17",
        "@langchain/groq": "^0.2.3",
        "@langchain/openai": "^0.6.9",
        "compression": "^1.7.4",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "express": "^4.19.2",
        "helmet": "^7.1.0",
        "ioredis": "^5.7.0",
        "openai": "^4.52.0",
        "pg": "^8.12.0",
        "redis": "^5.8.2",
        "uuid": "^10.0.0",
        "winston": "^3.13.0",
        "zod": "^4.1.3"
    },
    "devDependencies": {
        "@artemkdr/biome-base": "^0.0.2",
        "@artemkdr/tsconfig-base-bun": "^0.0.2",
        "@biomejs/biome": "^2.2.4",
        "@types/bun": "latest",
        "@types/compression": "^1.7.5",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^24.3.0",
        "@types/pg": "^8.11.6",
        "@types/uuid": "^10.0.0",
        "globals": "^16.3.0",
        "typescript": "^5.5.4"
    },
    "engines": {
        "bun": ">=1.0.0",
        "node": ">=18.0.0"
    },
    "keywords": [
        "express",
        "biome",
        "typescript",
        "bun"
    ],
    "author": "Artem Kudryavtsev",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/BCars-SA/knowledge.git"
    },
    "private": false
}
